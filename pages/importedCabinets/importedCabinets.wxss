
page{
}
.import-cabinet{
    position: relative;
}
.cabinet-img{
    width: 100%;
    display: block;
    margin-bottom: 20rpx;
}
.select-type{
    text-align: center;
}

.imgs-wrapper{
    padding: 5rpx 20rpx;
    width: 100%;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
}
.imgs-list-left, .imgs-list-right{
    width: 50%;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
}
.imgs-list-left{
    padding-right: 5rpx;
}
.imgs-list-right{
    padding-left: 5rpx;
}
.imgs-item{
    width: 100%;
    box-sizing: border-box;
    padding-bottom: 10rpx;
}
.imgs-content{
    width: 100%;
    display: block;
}


.btn{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 30rpx 20rpx;
}

.empty-wrapper{
    margin-top: 210rpx;
    text-align: center;
}
.empty-img{
    width: 130rpx;
}
.empty-msg{
    display: block;
    font-size: 30rpx;
    padding-top: 30rpx;
}