page{
  height: 100%;
  background-color: #eee;
}
.my-order{
  height: 100%;
  position: relative;
}
.tabs{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 6;
  width: 100%;
}
.list-wrapper{
  margin-top: 110rpx;
}
.list-item{
  background-color: #fff;
  width: 100%;
  margin-bottom: 20rpx;
}
.title{
  border-bottom: 1px solid #eee;
  padding: 25rpx 30rpx;
}
.left-title{

}
.right-title{
  color: #4D4D4D;
  font-size: 30rpx;
}
.content-b{
  padding: 30rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}
.content-item{
  color: #4D4D4D;
  font-size: 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.bottom{
  padding-top: 15rpx;
}

.footer-b{
  font-size: 30rpx;
  padding: 25rpx 30rpx;
}
.left-footer{
  font-size: 35rpx;
}
.right-footer{
  color: #4D4D4D;
  text-align: right;
}

.text-red{
  color: #FF0000;
}
.text-el{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.empty-wrapper{
  margin-top: 210rpx;
  text-align: center;
}
.empty-img{
  width: 130rpx;
}
.empty-msg{
  display: block;
  font-size: 30rpx;
  padding-top: 30rpx;
}

.authorize-warning{
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: 999;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box{
  width: 80%;
  display: block;
  background-color: #fff;
  border-radius: 10rpx 10rpx;
}
.title{
  font-size: 35rpx;
  font-weight: bold;
  text-align: center;
  border-bottom: 1px solid #ccc;
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx 10rpx;
}
.authorize{
  color: #009900;
  font-weight: bold;
  font-size: 35rpx;
  text-align: center;
  border-top: 1px solid #ccc;
  display: block;
  width: 100%;
  padding: 25rpx 10rpx;
  background-color: #fff;
  line-height: 1.6;
}
.authorize:after{
  border: none;
  padding: 15rpx 10rpx;
  color: #009900;
}
.content{
  text-align: center;
  padding: 30rpx;
}
.logo-img{
  width: 80rpx;
  height: 80rpx;
  background-color: #fff;
  border-radius: 50% 50%;
}
.content-tips1{
  text-align: left;
  font-size: 30rpx;
  color: #4D4D4D;
  border-bottom: 1px solid #ccc;
  padding: 20rpx;
}
.content-tips2{
  color: #888;
  font-size: 30rpx;
  position: relative;
  text-indent: 15rpx;
  text-align: left;
  padding: 20rpx;
}
.content-tips2:after{
  content: '';
  position: absolute;
  left: 10rpx;
  top: 35rpx;
  width: 10rpx;
  height: 10rpx;
  background-color: #888;
  border-radius: 50% 50%;
}
.authorize-warning-title{
  font-size: 35rpx;
  font-weight: bold;
  text-align: center;
  border-bottom: 1px solid #ccc;
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx 10rpx;
}