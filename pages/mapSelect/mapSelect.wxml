<view class="container map-select">
    <wux-search-bar clear maxlength="20" placeholder="搜索地点" bind:change="searchMap"/>
    <map class="map" longitude="{{longitude}}" latitude="{{latitude}}" include-points="{{points}}" markers='{{markers}}'></map>
    <view class="content">
        <text class="msg-detail">提交成功，请保持电话畅通</text>
        <text class="msg-detail">我们将于2小时内联系你</text>
        <text class="msg-detail">请耐心等待</text>
    </view>
    <view class="btn">
        <wux-button block type="calm" bind:click="go">返回</wux-button>
    </view>
</view>
