page{
  height: 100%;
}
.apply-success{
  background-color: #fff;
  height: 100%;
  position: relative;
  text-align: center;
}
.aps-img{
  margin-top: 120rpx;
  width: 130rpx;
}
.content{
  box-sizing: border-box;
  width: 100%;
  padding: 25rpx 30rpx 25rpx;
  display: block;
  margin-top: 35rpx;
}

.msg-detail{
  color: #808080;
  font-size: 28rpx;
  line-height: 1.7;
  display: block;
  font-weight: bold;
}
.btn{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 30rpx 20rpx;
}