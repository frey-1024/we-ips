page{
    height: 100%;
}
.apply-success{
    padding-top: 100rpx;
    text-align: center;
}
.aps-img{
    width: 180rpx;
}

.msg-detail{
    color: #4D4D4D;
    font-size: 26rpx;
    line-height: 1.7;
    display: block;
    font-weight: bold;
    margin-top: 60rpx;
}

.tips{
    border-top: 1px solid #E5E5E5;
    text-align: left;
    padding: 30rpx 30rpx;
    margin-top: 60rpx;
}
.tips-title, .tip-content{
    display: block;
    color: #4D4D4D;
    font-size: 26rpx;
    padding-bottom: 30rpx;
}
.tips-title{
    padding-bottom: 50rpx;
}

.btn{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 30rpx 20rpx;
}