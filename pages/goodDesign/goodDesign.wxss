page{
    height: 100%;
    background-color: #eee;
}
.good-design{
    height: 100%;
    position: relative;
    box-sizing: border-box;
}
.good-icons{
    padding: 40rpx 30rpx;
    background-color: #fff;
    text-align: center;
}
.good-item{
    padding-right: 40rpx;
    box-sizing: border-box;
}
.good-item:last-child{
    padding-right: 0;
}
.good-img{
    width: 100%;
    display: block;
}
.good-text{
    font-size: 30rpx;
    padding-top: 30rpx;
    display: block;
}
.swiper{
    width: 100%;
    box-sizing: border-box;
    height: 430rpx;
}
.good-liu{
    width: 100%;
    display: block;
    height: 430rpx;
}
.good-more{
    width: 100%;
    display: block;
}
.good-more-list{
    background-color: #fff;
    padding: 0 30rpx 20rpx;
    box-sizing: border-box;
    margin-bottom: 110rpx;
}
.bg-white{
    background-color: #fff;
}
.bg-blue{
    background-color: #6EB8F1;
}
.h-125{
    height: 260rpx;
}

.btn{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    box-sizing: border-box;
}
.btn-click{
    border-radius: 0;
    background-color: #6BF3D4;
    color: #fff;
    font-size: 32rpx;
    padding: 25rpx;
    text-align: center;
}
.btn-click-calm{
    background-color: #11c1f3;
}

.authorize-warning{
    position: fixed;
    background-color: rgba(0, 0, 0, 0.5);
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    z-index: 999;
    display: flex;
    justify-content: center;
    align-items: center;
}
.box{
    width: 80%;
    display: block;
    background-color: #fff;
    border-radius: 10rpx 10rpx;
}
.authorize-warning-title{
    font-size: 35rpx;
    font-weight: bold;
    text-align: center;
    border-bottom: 1px solid #ccc;
    display: block;
    width: 100%;
    box-sizing: border-box;
    padding: 25rpx 10rpx;
}
.authorize{
    color: #009900;
    font-weight: bold;
    font-size: 35rpx;
    text-align: center;
    border-top: 1px solid #ccc;
    display: block;
    width: 100%;
    padding: 25rpx 10rpx;
    background-color: #fff;
    line-height: 1.6;
}
.authorize:after{
    border: none;
    padding: 15rpx 10rpx;
    color: #009900;
}
.content{
    text-align: center;
    padding: 30rpx;
}
.logo-img{
    width: 80rpx;
    height: 80rpx;
    background-color: #fff;
    border-radius: 50% 50%;
}
.content-tips1{
    text-align: left;
    font-size: 30rpx;
    color: #4D4D4D;
    border-bottom: 1px solid #ccc;
    padding: 20rpx;
}
.content-tips2{
    color: #888;
    font-size: 30rpx;
    position: relative;
    text-indent: 15rpx;
    text-align: left;
    padding: 20rpx;
}
.content-tips2:after{
    content: '';
    position: absolute;
    left: 10rpx;
    top: 35rpx;
    width: 10rpx;
    height: 10rpx;
    background-color: #888;
    border-radius: 50% 50%;
}

.get-phone-btn{
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    opacity: 0;
    z-index: 3;
}