page{
    height: 100%;
    background-color: #eee;
}
.high-team{
    height: 100%;
    position: relative;
    box-sizing: border-box;
}
.my-provide, .my-promise{
    margin-top: 20rpx;
    background-color: #fff;
    padding: 20rpx 40rpx 0;
    text-align: center;
}
.provide-title, .promise-title{
    font-size: 35rpx;
    color: #4D4D4D;
    padding-bottom: 20rpx;
    display: block;
    text-align: center;
}
.provide-icons{
    padding-bottom: 20rpx;
}
.provide-item{
    padding-right: 90rpx;
    box-sizing: border-box;
}
.provide-item:last-child{
    padding-right: 0;
}
.provide-img{
    width: 100%;
    display: block;
}
.provide-tip, .promise-item{
    font-size: 28rpx;
    color: #4D4D4D;
    line-height: 1.6;
    padding-bottom: 30rpx;
    text-align: left;
}
.provide-tip{
    display: block;
}
.my-promise{
    text-align: left;
    margin-top: 10rpx;
}


.rongyu-img{
    width: 100%;
    display: block;
    margin-top: 10rpx;
    margin-bottom: 150rpx;
}


.btn{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 30rpx 20rpx;
}

.get-phone-btn{
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    opacity: 0;
    z-index: 3;
}

.authorize-warning{
    position: fixed;
    background-color: rgba(0, 0, 0, 0.5);
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    z-index: 999;
    display: flex;
    justify-content: center;
    align-items: center;
}
.box{
    width: 80%;
    display: block;
    background-color: #fff;
    border-radius: 10rpx 10rpx;
}
.title{
    font-size: 35rpx;
    font-weight: bold;
    text-align: center;
    border-bottom: 1px solid #ccc;
    display: block;
    width: 100%;
    box-sizing: border-box;
    padding: 25rpx 10rpx;
}
.authorize{
    color: #009900;
    font-weight: bold;
    font-size: 35rpx;
    text-align: center;
    border-top: 1px solid #ccc;
    display: block;
    width: 100%;
    padding: 25rpx 10rpx;
    background-color: #fff;
    line-height: 1.6;
}
.authorize:after{
    border: none;
    padding: 15rpx 10rpx;
    color: #009900;
}
.content{
    text-align: center;
    padding: 30rpx;
}
.logo-img{
    width: 80rpx;
    height: 80rpx;
    background-color: #fff;
    border-radius: 50% 50%;
}
.content-tips1{
    text-align: left;
    font-size: 30rpx;
    color: #4D4D4D;
    border-bottom: 1px solid #ccc;
    padding: 20rpx;
}
.content-tips2{
    color: #888;
    font-size: 30rpx;
    position: relative;
    text-indent: 15rpx;
    text-align: left;
    padding: 20rpx;
}
.content-tips2:after{
    content: '';
    position: absolute;
    left: 10rpx;
    top: 35rpx;
    width: 10rpx;
    height: 10rpx;
    background-color: #888;
    border-radius: 50% 50%;
}
.authorize-warning-title{
    font-size: 35rpx;
    font-weight: bold;
    text-align: center;
    border-bottom: 1px solid #ccc;
    display: block;
    width: 100%;
    box-sizing: border-box;
    padding: 25rpx 10rpx;
}